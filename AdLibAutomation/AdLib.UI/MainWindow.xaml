<Window x:Class="AdLib.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AdLib Automation" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Content="Load Plugins" Click="LoadPlugins_Click" Margin="10"/>
            <Button Content="Execute Plugins" Click="ExecutePlugins_Click" Margin="10"/>
            <TextBox Name="OutputTextBox" Margin="10" Height="200" IsReadOnly="True" TextWrapping="Wrap"/>
        </StackPanel>
    </Grid>
</Window>

    